{
  "name": "dev-demand",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": "cd server && npm run start",
    "seed": "cd server && npm run seed",
    "cleandb": "cd server && npm run cleandb",
    "develop": "concurrently \"npm run server:dev\" \"wait-on tcp:3001 && npm run client:dev\"",
    "server:dev": "cd server && npm run watch",
    "client:dev": "cd client && npm run dev",
    "install": "cd server && npm i && cd ../client && npm i",
    "build": "concurrently \"cd server && npm run build\" \"cd client && npm run build\"",
    "render-build": "npm install && npm run build",
    "test": "npx cypress run --browser electron",
    "test-component": "npx cypress run --component",
    "test-gui": "npx cypress open"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@cypress/vite-dev-server": "^5.2.1",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.0",
    "cypress": "^13.16.0",
    "nodemon": "^3.1.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.11.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3",
    "vitest": "^2.1.5",
    "wait-on": "^8.0.1"
  },
  "engines": {
    "node": "20.x"
  },
  "dependencies": {
    "@cypress/react18": "^2.0.1",
    "@testing-library/cypress": "^10.0.2"
  }
}
